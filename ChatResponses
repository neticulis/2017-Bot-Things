		let m2 = data.message.toLowerCase(); // lowercase message with @ INCLUDED
		m = m.replace('@', '');
		M = M.replace('@', '');
		if (player.username == bot.masterbot && data.username == "MathWins") {
			// Master bot running, math sent message, lets see if it is a command

