					let shit = bot.shitlist.toString();
					bot.say(`Shitlist (Blacklist): ${shit}`);
					return null
				}
				if (listToList == 'trusted' || listToList == 'trusted users' || listToList == 'trust') {
					// LIST TRUSTED USERS
					let trus = bot.trustedUsers.toString();
					bot.say(`Trusted Users: ${trus}`);
					return null
